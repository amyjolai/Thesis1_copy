  url      = {http://www.sciencedirect.com/science/article/pii/0032386181903013},
}

@Article{Cossi2003,
  author    = {Cossi, Maurizio and Rega, Nadia and Scalmani, Giovanni and Barone, Vincenzo},
  journal   = {J. Comput. Chem.},
