@Article{Binke1999,
  author   = {Binke, N. and Rong, L. and Zhengquan, Y. and Yuan, W. and Pu, Y. and Rongzu, Hu and Qingsen, Y.},
  journal  = {Journal of Thermal Analysis and Calorimetry},
  title    = {Studies on the Kinetics of the First Order Autocatalytic Decomposition Reaction of Highly Nitrated Nitrocellulose},
  year     = {1999},
