file = {:C$\backslash$:/Users/amyjo/AppData/Local/Mendeley Ltd/Mendeley Desktop/Downloaded/norwood-1991-slurries as supplemental fuels.pdf:pdf},
institution = {US Army Corps of Engineers},
pages = {1--101},
title = {{TECHNICAL AND ECONOMIC ANALYSES TO ASSESS THE FEASIBILITY OF -USING PROPELLANT- NO. 2 FUEL OIL SLURRIES AS SUPPLEMENTAL FUELS}},
year = {1991}
}
