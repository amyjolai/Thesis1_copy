file = {:C$\backslash$:/Users/amyjo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Jin et al. - 2015 - The thermal decomposition mechanism of nitrocellulose aerogel.pdf:pdf},
