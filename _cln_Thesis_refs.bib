keywords = {CL-20,COMBUSTION,DEGRADATION,Decomposition mechanism,ENERGETIC MATERIALS,KINETICS,Nitrocellulose aerogel,Porous structure,TG/DSC/FTIR},
month = {mar},
number = {2},
pages = {901--908},
publisher = {SPRINGER, VAN GODEWIJCKSTRAAT 30, 3311 GZ DORDRECHT, NETHERLANDS},
title = {{The thermal decomposition mechanism of nitrocellulose aerogel}},
url = {http://apps.webofknowledge.com/full{\_}record.do?product=UA{\&}search{\_}mode=GeneralSearch{\&}qid=2{\&}SID=R2T8ytlHVN9acp1iGsw{\&}page=1{\&}doc=1{\&}cacheurlFromRightClick=no http://link.springer.com/10.1007/s10973-015-4574-4},
