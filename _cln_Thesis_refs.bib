doi = {10.1016/j.jaap.2014.08.002},
file = {:C$\backslash$:/Users/amyjo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lu et al. - 2014 - The mechanism for the formation of levoglucosenone during pyrolysis of $\beta$-d-glucopyranose and cellobiose A density fun.pdf:pdf},
issn = {01652370},
journal = {Journal of Analytical and Applied Pyrolysis},
