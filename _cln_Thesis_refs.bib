issn = {0165-9936},
journal = {TrAC Trends in Analytical Chemistry},
month = {dec},
number = {11},
pages = {1740--1755},
publisher = {Elsevier},
title = {{Analytical techniques in the study of highly-nitrated nitrocellulose}},
