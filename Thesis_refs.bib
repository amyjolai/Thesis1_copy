file = {:C$\backslash$:/Users/amyjo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gonz{\'{a}}lez et al. - 2006 - Bacterial nitrate reductases Molecular and biological aspects of nitrate reduction.pdf:pdf},
issn = {0162-0134},
journal = {Journal of Inorganic Biochemistry},
month = {may},
number = {5-6},
pages = {1015--1023},
publisher = {Elsevier},
title = {{Bacterial nitrate reductases: Molecular and biological aspects of nitrate reduction}},
url = {https://www.sciencedirect.com/science/article/pii/S0162013405003314?via{\%}3Dihub},
volume = {100},
year = {2006}
}
