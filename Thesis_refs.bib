journal = {LLNL-TR-415057, Lawrence Livermore National Laboratory, Livermore, CA, USA},
keywords = {activation energy,aging,ambient temperature,chemical analysis,chemical properties,crystal growth,crystal structure,diffusion,distribution,exploding wires,impurities,inorganic,moisture,morphology,organic,particle size,petn,physical and analytical chemistry,pyrolysis,specific surface area,surface area,thermal analysis,thermal degradation},
month = {apr},
title = {{Aging of pentaerythritol tetranitrate (PETN)}},
