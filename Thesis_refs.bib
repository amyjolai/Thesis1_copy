@article{Lu2014,
abstract = {Levoglucosenone (LGO) is a valuable anhydrosugar product from fast pyrolysis of cellulose/biomass, but its formation mechanism is still unclear at present. In this study, density functional theory (DFT) method was employed to examine the different LGO formation pathways from pyrolysis of both $\beta$-d-glucopyranose and cellobiose, and the most favorable pathways were clarified. The results corroborate the previous studies that levoglucosan (LG) is unlikely the essential intermediate to produce LGO. The most feasible LGO formation pathways from both $\beta$-d-glucopyranose and cellobiose include sequential 1,2-dehydration, six-membered hydrogen transfer and enol-keto tautomerization steps. Enol-keto tautomerization is the rate-determining steps for both reactions to form LGO.},
author = {Lu, Qiang and Zhang, Yang and Dong, Chang-qing and Yang, Yong-ping and Yu, Hai-zhu},
doi = {10.1016/j.jaap.2014.08.002},
file = {:C$\backslash$:/Users/amyjo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lu et al. - 2014 - The mechanism for the formation of levoglucosenone during pyrolysis of $\beta$-d-glucopyranose and cellobiose A density fun.pdf:pdf},
issn = {01652370},
journal = {Journal of Analytical and Applied Pyrolysis},
keywords = {Cellobiose,DFT,Levoglucosenone,Pyrolysis mechanism,$\beta$-d-glucopyranose},
month = {nov},
pages = {34--43},
title = {{The mechanism for the formation of levoglucosenone during pyrolysis of $\beta$-d-glucopyranose and cellobiose: A density functional theory study}},
url = {http://www.sciencedirect.com/science/article/pii/S0165237014001806},
volume = {110},
year = {2014}
}
