pages = {8307--8320},
publisher = {Copernicus GmbH},
title = {{Thermodynamics and kinetics of the hydrolysis of atmospherically relevant organonitrates and organosulfates}},
url = {http://www.atmos-chem-phys.net/11/8307/2011/},
volume = {11},
year = {2011}
