@article{Cao2019,
abstract = {Different DFT methods give relative energies for models of the FeMo cluster in nitrogenase that differ by up to 600 kJ mol −1 . This is 4–30 times more than what is observed for other systems and explain why previous computational studies have given conflicting results. It is caused by differences in the oxidation state of the metals and the number of Fe–C, Fe–S and Fe–H bonds, connected to the amount of static correlation. Nitrogenase is the only enzyme that can cleave the triple bond in N 2 , making nitrogen avaiable for other organisms. It contains a complicated MoFe 7 S 9 C(homocitrate) cluster in its active site. Many computational studies with density-functional theory (DFT) of the nitrogenase enzyme have been presented, but they do not show any consensus – they do not even agree where the first four protons should be added, forming the central intermediate E 4 . We show that the prime reason for this is that different DFT methods give relative energies that differ by almost 600 kJ mol −1 for different protonation states. This is 4–30 times more than what is observed for other systems. The reason for this is that in some structures, the hydrogens bind to sulfide or carbide ions as protons, whereas in other structures they bind to the metals as hydride ions, changing the oxidation state of the metals, as well as the Fe–C, Fe–S and Fe–Fe distances. The energies correlate with the amount of Hartree–Fock exchange in the method, indicating a variation in the amount of static correlation in the structures. It is currently unclear which DFT method gives the best results for nitrogenase. We show that non-hybrid DFT functionals and TPSSh give the most accurate structures of the resting active site, whereas B3LYP and PBE0 give the best H 2 dissociation energies. However, no DFT method indicates that a structure of E 4 with two bridging hydride ions is lowest in energy, as spectroscopic experiments indicate.},
annote = {Good paper for reference for presentation of computational reaction data},
author = {Cao, Lili and Ryde, Ulf},
doi = {10.1039/c8cp06930a},
file = {:C$\backslash$:/Users/amyjo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Cao, Ryde - 2019 - Extremely large differences in DFT energies for nitrogenase models.pdf:pdf},
issn = {14639076},
journal = {Physical Chemistry Chemical Physics},
month = {jan},
number = {5},
pages = {2480--2488},
publisher = {The Royal Society of Chemistry},
title = {{Extremely large differences in DFT energies for nitrogenase models}},
url = {http://xlink.rsc.org/?DOI=C8CP06930A},
volume = {21},
year = {2019}
}
@article{Chin2007,
abstract = {Nitrocellulose based (NC) and nitroglycerin based (NG) propellants often have a fixed acid and water content during the manufacturing time. After manufacture, the quantity and ratio of acid/water will continue to vary depending upon the conditions of storage and operation. The level of variation depends on many factors such as loading density, temperature, volume of ullage and sealing condition of the containing cartridge, just to name a few. As described in this paper and other literature, the degradation mechanisms and aging processes of NC/NG based propellants are extremely complicated. This paper describes the details of the application of Electron Spin Resonance (ESR) to study if the free-radical mechanism is involved in the decomposition of nitrocellulose and nitroglycerin. Due to the high free-radical intensity possessed by the propellant composition, we believe that a a complex intermediate may be formed between DPA and NG and/or NC. The formation of a it complex intermediate is not preferred because it may enhance the rate of decomposition of nitrate esters. Correlation between heat flow curve, stabilizer depletion and free-radical distribution has been evaluated. An intramolecular decomposition mechanism is preferred, because it prevents the possibility that the NOx from NNODPA or NNDPA may interact with the NC and NG again during the rearrangement process. Since the acidity of the propellant composition continues to change during the aging process, multiple mechanisms may proceed simultaneously.},
annote = {READ ME},
author = {Chin, Anton and Ellison, Daniel S. and Poehlein, Sara K. and Ahn, Myong K.},
doi = {10.1002/prep.200700013},
file = {:C$\backslash$:/Users/amyjo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Chin et al. - 2007 - Investigation of the Decomposition Mechanism and Thermal Stability of NitrocelluloseNitroglycerine Based Propellant.pdf:pdf},
issn = {07213115},
journal = {Propellants, Explosives, Pyrotechnics},
keywords = {NITROCELLULOSE,electron spin resonance,nitrocellulose,nitroglycerin,propellants,stablizer},
month = {apr},
number = {2},
pages = {117--126},
publisher = {WILEY-V C H VERLAG GMBH, PO BOX 10 11 61, D-69451 WEINHEIM, GERMANY},
title = {{Investigation of the Decomposition Mechanism and Thermal Stability of Nitrocellulose/Nitroglycerine Based Propellants by Electron Spin Resonance}},
url = {http://onlinelibrary.wiley.com/doi/10.1002/prep.200700013/abstract},
volume = {32},
year = {2007}
}
