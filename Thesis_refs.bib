journal = {Atmospheric Chemistry and Physics},
month = {oct},
number = {20},
pages = {15419--15436},
title = {{A comprehensive organic nitrate chemistry: Insights into the lifetime of atmospheric organic nitrates}},
url = {https://www.atmos-chem-phys.net/18/15419/2018/},
volume = {18},
year = {2018}
}
