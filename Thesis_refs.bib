keywords = {NITROCELLULOSE,electron spin resonance,nitrocellulose,nitroglycerin,propellants,stablizer},
month = {apr},
number = {2},
pages = {117--126},
publisher = {WILEY-V C H VERLAG GMBH, PO BOX 10 11 61, D-69451 WEINHEIM, GERMANY},
