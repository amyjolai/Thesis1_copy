doi = {10.1021/ja037055w},
file = {:C$\backslash$:/Users/amyjo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Nishiyama et al. - 2003 - Crystal structure and hydrogen bonding system in cellulose I(alpha) from synchrotron X-ray and neutron fiber d.pdf:pdf},
