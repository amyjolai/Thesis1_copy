  affiliation   = {Army Natick Research and Development Command, MA (USA)},
  descriptors   = {USINE CHIMIQUE; CELLULOSE NITRATE; EPURATION EAU USEE; EPURATION PHYSICOCHIMIQUE; EPURATION BIOLOGIQUE; DENITRIFICATION; BOUE ACTIVEE; NUISANCES; POLLUTION},
