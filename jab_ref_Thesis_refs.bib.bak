@Article{ElDiwani2009,
  author   = {El-Diwani, G. and El-Ibiari, N. N. and Hawash, S. I.},
  journal  = {Journal of Hazardous Materials},
  title    = {Treatment of hazardous wastewater contaminated by nitrocellulose},
  year     = {2009},
  issn     = {0304-3894},
  number   = {1},
  pages    = {830--834},
  volume   = {167},
  abstract = {Based on successful preliminary bench scale experimental studies for treatment of industrial wastewater contaminated by nitrocellulose, a pilot plant is constructed for results assessment. Bench scale experimental work proved 55%, 73% recovery of nitrocellulose without and with chemical addition respectively within 10min flotation compared to 35%, 69% recovery within 2-0.5h settling respectively. The treatment process aims the recovery of nitrocellulose through an efficient dissolved air flotation (DAF) unit. Different operating conditions have been studied for different effluent characteristics with and without flocculating agent. Nitrocellulose recovery reached 80% by flotation without chemical, which is increased to 87% using cationic polymer, but both exploring suitable pathways to solve the recovery problem.The experimental results are considered suitable basis for full scale design of the industrial treatment unit.},
  doi      = {10.1016/j.jhazmat.2009.01.063},
  keywords = {Hazardous waste, Nitrocellulose, Treatment, Pilot plant},
  url      = {http://www.sciencedirect.com/science/article/pii/S0304389409000995},
}

@InCollection{Quinn2015,
  author    = {Quinn, Michael J. and Williams, Marc A. and Reddy, Gunda and Quinn, Michael J. and Johnson, Mark S.},
  booktitle = {Wildlife Toxicity Assessments for Chemicals of Military Concern},
  publisher = {Elsevier},
  title     = {Chapter 11 - Wildlife Toxicity Assessment for Nitrocellulose},
  year      = {2015},
  pages     = {217--226},
  abstract  = {Nitrocellulose (also known as gun cotton) is a highly flammable white, fibrous, pulp-like compound formed by nitrating starch or wood fibers. It is widely used by the military in explosives, smokeless powders, mortar increments, rocket fuels, and propellants. It is virtually insoluble in water; thus surface and groundwater contamination is unlikely. In chronic, subchronic, and multigeneration studies nitrocellulose was non-toxic to rats and dogs; however, mortality was seen in mice, which was attributed to the relatively large size of the fibers relative to the size of the murine intestinal lumen rather than to chemical toxicity. Oral LD50 values are greater than 5 grams per kilogram (g/kg) in rats. The ingestion toxicity reference value (TRV) for the class Mammalia is considered non-toxic. Further, a lack of toxicity data for hindgut fermenting species, birds, reptiles, and amphibians precludes development of a TRV. Thus, nitrocellulose is likely to be non-toxic for most monogastric mammalian species.},
  doi       = {10.1016/B978-0-12-800020-5.00011-9},
  issn      = {978-0-12-800020-5},
  keywords  = {Nitrocellulose, CAS No. 9004-70-0, toxicity, wildlife, Toxicity Reference Value, TRV},
  url       = {http://www.sciencedirect.com/science/article/pii/B9780128000205000119},
}

@Article{Duran1994,
  author   = {Duran, M. and Kim, B. J. and Speece, R. E.},
  journal  = {Waste Management},
  title    = {Anaerobic biotransformation of nitrocellulose},
  year     = {1994},
  issn     = {0956-053X},
  number   = {6},
  pages    = {481--487},
  volume   = {14},
  abstract = {In this investigation, first the toxic effect of nitrocellulose (NC) on anaerobic microorganisms with and without the presence of a supplementary carbon source was evaluated. Then it was observed that with the addition of a supplementary carbon source, such as cellulose or acetate, biotransformation of NC was enhanced. The biodegradation rate of NC in a staged-feed and in a two-phase reactor under anaerobic conditions was evaluated. The results showed that providing cellulose as the supplementary carbon source, NC was degraded at a higher rate in a staged-feed reactor than in a two-phase reactor or in a batch reactor.},
  doi      = {10.1016/0956-053X(94)90132-5},
  url      = {http://www.sciencedirect.com/science/article/pii/0956053X94901325},
}

