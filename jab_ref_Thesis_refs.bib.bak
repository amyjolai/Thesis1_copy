@Article{Morfa2016,
  author   = {Morfa, A. J. and Rödlmeier, T. and Jürgensen, N. and Stolz, S. and Hernandez-Sosa, G.},
  journal  = {Cellulose},
  title    = {Comparison of biodegradable substrates for printed organic electronic devices},
  year     = {2016},
  issn     = {1572-882X},
  number   = {6},
  pages    = {3809--3817},
  volume   = {23},
  abstract = {Building on the results of the green chemistry movement, the development of biodegradable strain sensors and OLEDs, produced from sustainable materials and solvents, is presented. The choice of solvents and substrate is discussed in the context of terms relevant to printing, namely solvent solubility parameters, surface wetting envelopes and surface roughness. A new method for producing biodegradable and flat substrates from a common and commercially available cellulose diacetate foil is presented. Challenges associated with working with biodegradable foils are presented and different techniques are discussed to ultimately overcome these challenges and produce functional devices. Lastly, many green solvents and several commercially available biodegradable foils are compared for consideration in future work.},
  doi      = {10.1007/s10570-016-1049-0},
  refid    = {Morfa2016},
  url      = {https://doi.org/10.1007/s10570-016-1049-0},
}

