@Article{Golubev2017,
  author   = {Golubev, A. E. and Neshitova, A. N. and Singin, P. V. and Kuvshinova, S. A. and Burmistrov, V. A. and Koifman, O. I.},
  journal  = {Russian Journal of General Chemistry},
  title    = {Plasticization of Cellulose Nitrate by 4-Nitrophtalic Acid Esters},
