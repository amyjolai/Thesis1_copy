@Article{Akerholm2004,
  author   = {Margaretha Akerholm and Barbara Hinterstoisser and Lennart Salmen},
  journal  = {Carbohydrate Research},
  title    = {Characterization of the crystalline structure of cellulose using static and dynamic FT-IR spectroscopy},
  year     = {2004},
  issn     = {0008-6215},
  number   = {3},
  pages    = {569--578},
  volume   = {339},
  abstract = {The cellulose structure is a factor of major importance for the strength properties of wood pulp fibers. The ability to characterize small differences in the crystalline structures of cellulose from fibers of different origins is thus highly important. In this work, dynamic FT-IR spectroscopy has been further explored as a method sensitive to cellulose structure variations. Using a model system of two different celluloses, the relation between spectral information and the relative cellulose I$\alpha$ content was investigated. This relation was then used to determine the relative cellulose I$\alpha$ content in different pulps. The estimated cellulose I allomorph compositions were found to be reasonable for both unbleached and bleached chemical pulps. In addition, it was found that the dynamic FT-IR spectroscopy technique had the potential to indicate possible correlation field splitting peaks of cellulose I$\alpha$.},
  doi      = {10.1016/j.carres.2003.11.012},
  keywords = {2D FT-IR, Cellulose allomorphs, Crystallinity, Infrared spectroscopy, Chemical pulps},
  url      = {http://www.sciencedirect.com/science/article/pii/S0008621503006104},
}

