issn = {1758-2946},
journal = {Journal of Cheminformatics},
keywords = {Computational Biology/Bioinformatics,Computer Applications in Chemistry,Documentation and Information in Chemistry,Theoretical and Computational Chemistry},
month = {dec},
number = {1},
pages = {17},
