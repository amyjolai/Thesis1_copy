file = {:C$\backslash$:/Users/amyjo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kimberly A. Jebber et al. - 1996 - Ab Initio and Experimental Studies on the Protonation of Glucose in the Gas Phase.pdf:pdf},
