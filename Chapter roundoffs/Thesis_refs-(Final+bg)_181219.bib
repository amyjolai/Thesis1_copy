file = {:C$\backslash$:/Users/amyjo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Nguyen et al. - 2012 - Direct aqueous photochemistry of isoprene high-NOx secondary organic aerosol.pdf:pdf},
issn = {14639076},
journal = {Physical Chemistry Chemical Physics},
month = {jun},
number = {27},
pages = {9702--9714},
publisher = {Royal Society of Chemistry},
title = {{Direct aqueous photochemistry of isoprene high-NO x secondary organic aerosol}},
url = {http://xlink.rsc.org/?DOI=c2cp40944e},
volume = {14},
