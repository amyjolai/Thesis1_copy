journal = {Journal of the American Chemical Society},
number = {43},
pages = {10515--10524},
publisher = {American Chemical Society},
title = {{Ab initio and experimental studies on the protonation of glucose in the gas phase}},
url = {https://pubs.acs.org/doi/full/10.1021/ja960427z},
volume = {118},
year = {1996}
}
