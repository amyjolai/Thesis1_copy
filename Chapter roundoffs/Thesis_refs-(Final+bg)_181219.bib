keywords = {aging,and national defense,chemical explosives,decomposition,experimental data,military technology,petn,thermal degradation,weaponry},
month = {nov},
title = {{Degradation chemistry of PETN and its homologues}},
url = {http://www.osti.gov/servlets/purl/650196-rV1bU0/webviewable/},
year = {1997}
}
