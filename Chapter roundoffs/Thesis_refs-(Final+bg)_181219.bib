file = {:C$\backslash$:/Users/amyjo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Schnupf et al. - 2007 - DFT study of $\alpha$- and $\beta$-d-allopyranose at the B3LYP6-311G  level of theory.pdf:pdf},
issn = {00086215},
journal = {Carbohydrate Research},
keywords = {Allose,B3LYP/6-311++G**,Boat,Chair,Conformation,Hessian,Relative free energy,Skew},
number = {2},
pages = {196--216},
title = {{DFT study of $\alpha$- and $\beta$-d-allopyranose at the B3LYP/6-311++G** level of theory}},
volume = {342},
year = {2007}
